<div class="card col-sm-6">
  <div class="card-header">
    <h2 class="display-4">Editing Photo</h2>
  </div>
  <div class="card-body">

<%= render 'form', photo: @photo %>
<%= image_tag @photo.image.url(:medium) %> <hr>
<%= link_to 'Show', @photo, class: "btn btn-primary" %> |
<%= link_to 'Destroy', @photo, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-primary" %> |
<%= link_to 'Back', photos_path, class: "btn btn-primary" %>
</div>
</div>
